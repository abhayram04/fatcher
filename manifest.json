{
  "manifest_version": 2,
  "name": "Fatcher - Report Fake News",
  "short_name": "Fatcher",
  "version": "1.3",
  "icons": {
    "48": "assets/img/ff4-logo.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": ["tabs", "identity", "identity.email"],
  "browser_action": {
    "default_icon": "assets/img/ff4-logo.png",
    "default_title": "Fatcher",
    "default_popup": "app.html"
  }
}
