{
	"manifest_version": 2,
	"name": "Fatcher",
	"version": "0.1",
	"icons": {
		"48": "assets/img/fl2.png"
	},
	"permissions": [
		"tabs",
		"identity",
		"identity.email"
	],
	"browser_action": {
		"default_icon": "assets/img/fl2.png",
		"default_title": "Fatcher",
		"default_popup": "app.html"
	}
}