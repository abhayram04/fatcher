{
	"manifest_version": 2,
	"name": "Fatcher",
	"version": "0.1",
	"content_security_policy": "script-src 'self' https://cdn.firebase.com https://www.gstatic.com; object-src 'self'",
	"icons": {
		"48": "assets/img/fl2.png"
	},
	"permissions": [
		"tabs",
		"identity",
		"identity.email",
		"https://github.com/login/oauth/access_token",
		"storage"
	],
	"content_scripts":[{
		"matches":["https://github.com/robots.txt*"],
		"js":["libs/chrome-ex-oauth2/injection.js"],
		"run_at":"document_start"
	}],
	"browser_action": {
		"default_icon": "assets/img/fl2.png",
		"default_title": "Fatcher",
		"default_popup": "app.html"
	},
	"oauth2": {
		"client_id": "977698507477-69l4mqu7h1jish646t7r4rv8t3ud41r3.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/userinfo.email"
		]
	},
	"key": ""
}